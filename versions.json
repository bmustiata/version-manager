{
  "version" : {
    "version": "$(cat package.json | grep '\"version\"' | cut -f4 -d\\\")",
    "files": {
      "README.md": [
        "^(# version-manager )(.*?)$"
      ]
    }
  },

  "yolo": {
    "version": "swag",
    "files": {
      "README.md" : {
        "match": "notevenonce ##VERSION##",
        "count": 0
      }
    }
  }
}

